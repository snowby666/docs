{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Electron Hub API",
  "logo": {
    "dark": "/logo/electronhub-text.png",
    "light": "/logo/electronhub-text.png"
  },
  "favicon": "/logo/electron.png",
  "colors": {
    "primary": "#8B5CF6",
    "light": "#A855F7",
    "dark": "#7C3AED",
    "anchors": {
      "from": "#8B5CF6",
      "to": "#A855F7"
    }
  },
  "topbarLinks": [
    {
      "name": "Playground",
      "url": "https://playground.electronhub.ai"
    },
    {
      "name": "Discord",
      "url": "https://discord.com/invite/electronhub"
    }
  ],
  "topbarCtaButton": {
    "name": "Get API Key",
    "url": "https://playground.electronhub.ai/console"
  },
  "api": {
    "baseUrl": [
      "https://api.electronhub.ai/v1",
      "https://api.electronhub.ai"
    ],
    "auth": {
      "method": "bearer",
      "name": "Authorization",
      "inputPrefix": "Bearer "
    },
    "playground": {
      "mode": "show"
    }
  },
  "openapi": "/api-reference/openapi.json",
  "navigation": [
    {
      "group": "Getting Started",
      "icon": "rocket",
      "pages": [
        "getting-started/introduction",
        "getting-started/quickstart",
        "getting-started/authentication",
        "getting-started/rate-limits",
        "getting-started/errors"
      ]
    },
    {
      "group": "API Reference",
      "icon": "code",
      "pages": [
        {
          "group": "Models",
          "icon": "brain",
          "pages": [
            "api-reference/models",
            "api-reference/model"
          ]
        },
        {
          "group": "Chat",
          "icon": "message",
          "pages": [
            "api-reference/chat/completions"
          ]
        },
        "api-reference/messages",
        "api-reference/responses",
        {
          "group": "Images",
          "icon": "image",
          "pages": [
            "api-reference/images/generations",
            "api-reference/images/edits"
          ]
        },
        {
          "group": "Videos",
          "icon": "video",
          "pages": [
            "api-reference/videos/generations"
          ]
        },
        {
          "group": "Audio",
          "icon": "volume-high",
          "pages": [
            "api-reference/audio/speech"
          ]
        },
        "api-reference/moderations",
        "api-reference/embeddings",
        {
          "group": "User",
          "icon": "user",
          "pages": [
            "api-reference/usage",
            "api-reference/user-logs"
          ]
        },
        {
          "group": "Proxy Keys",
          "icon": "key",
          "pages": [
            "api-reference/proxy-keys",
            "api-reference/proxy-create",
            "api-reference/proxy-update",
            "api-reference/proxy-toggle",
            "api-reference/proxy-delete",
            "api-reference/proxy-delete-all",
            "api-reference/proxy-validate"
          ]
        },
        "api-reference/regenerate-key"
      ]
    },
    {
      "group": "Guides",
      "icon": "book-open",
      "pages": [
        "guides/best-practices",
        "guides/openai-sdks",
        "guides/anthropic-sdks",
        "guides/prompt-caching",
        "guides/in-chat-image-generation"
